{
  "name": "bdist2-3d",
  "description": "Comparison of results with LB3D, using the BDist2 equilibrium distribution function",
  "tags": [ "advection", "collision", "eqdist" ],

  "configuration": "d3q19",
  "input-file": "bdist2-3d.in",
  "clean": [ "output" ],
